package com.shinhan.day01;

public class VariableVariation {
	static int score = 300; //static이 없으면 메모리에 안올라온다.

	public static void main(String[] args) {
		System.out.println("class 변수접근 "+score); //static이 있어야 읽을 수 있
		int score = 100;
		method1();
		System.out.println("main에서 출력 " + score);
		System.out.println("main에서 출력 " + 변수의범위.score);
	}

	private static void method1() {
		int score = 200;
		System.out.println("method1에서출력 "+score);
	} //함수를 부르고 나서 함수가 지워진다.

}
