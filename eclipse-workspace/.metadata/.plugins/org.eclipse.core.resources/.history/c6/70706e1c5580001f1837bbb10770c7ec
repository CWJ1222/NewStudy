package com.shinhan.day05;

public class ComputerTest {

	public static void main(String[] args) {
		f1();
	}

	private static void f1() {
		// 기본형이었다면 선언 후 초기화 후 사용하면 됨
		int a;
		a = 10;
		++a;
		System.out.println(a);
		// 객체 참조변수를 선언
		Computer c1;
		// 객체생성
		c1 = new Computer();
		// 객체사용
		System.out.println(c1);
	}

}
