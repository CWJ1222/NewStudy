package com.shinhan.day02;

public class 예제44 {

	public static void main(String[] args) {
		call();
	}

	private static void call() {
while(true) {
	int su1 = (int) (Math.random() *6 ) +1);
	int su2 = (int) (Math.random() *6 ) +1);
	System.out.println(su1 + "---" + su2);
	if(su1 + su2 == 5)
		break;
	
}
	}

}
