package com.shinhan.day02;

import java.io.PrintStream;

/*
 * 
 * java.base 모듈의 java.lang패키지는 import없이 사용
 * 나머지 클래스 등은 반드시 import한다.
 * 
 */
public class 표준입출력 {

	public static void main(String[] args) {
	  print();

	}

	private static void print() {
		//표준출력 : 기본은 모니터로 되어 있음
	PrintStream a = System.out;
	a.print(10);
	a.print("20");
	a.print(false);
	a.print(a);
	}

}
