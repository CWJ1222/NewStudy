package com.shinhan.day02;

public class 예제7 {

	public static void main(String[] args) {
		call1();
	}

	private static void call1() {
		for (int i = 1; i <= 5; i++) {
			for (int j = 1; j <= i; j++) {
				System.out.print("■");
			}
			System.out.println();
		}
	}
}



/*
 * 
 * 1줄 1 ~1칸 2줄 1칸 ~2칸 5줄 1칸 ~5칸
