package com.shinhan.day03;

public class ReferenceTest {

	public static void main(String[] args) {
f1();

	

	private static void f1() {
		// 기본형이 아니면 참조형이다.
		int a = 10;//stack에 저장
		//Integer b = new Integer(20); 지금은 이렇게 안쓴
		Integer b = 20; //기본형기능(값할당, 연산, 비교) + 기타기능 .... Heap영역에 저장됨
		
		System.out.println(b.compareTo(30));
		System.out.println(b.compareTo(10));
		
		
	}

}