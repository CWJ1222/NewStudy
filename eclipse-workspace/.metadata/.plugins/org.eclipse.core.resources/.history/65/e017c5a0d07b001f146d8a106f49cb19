package com.shinhan.day02;

public class 예제4 {
	public static void main(String[] args) {
		call1();

	}

	private static void call1() {
		//무작위 실수 : 0보다 크거나 같고 ~ 1보다 작다. 0.????
			System.out.println(Math.PI);
			System.out.println(Math.ceil(10.1)); //무조건 올림
			System.out.println(Math.floor(10.1)); //무조건 내림
			System.out.println(((int)(Math.random()*45)+1)); //랜덤(0~1 사이에 있음
			
			
	
			
	}

}
