	private static void q4() {
		byte a = 10, b = 20, c;
		c = (byte) (a + b);
		System.out.println(c);
	}

	/*
	 * private static void q1() { int var1; // 변수선언 //char var1 --- 변경불가 //var1 =
	 * var1 + 20 할당안되어 사용안됨 var1 = 10; var1 = var1 + 20; System.out.println(var1); }
	 */
	/*
