	private static void f2() {
		int [] array = {1, 5, 3, 8, 2};
		int max = Integer.MIN_VALUE;
		for(int su : array)
			{
				max = su > max ? su : max;
			}
			System.out.println(max);
		}
	

	private static void f1() {
		int[] array = { 1, 5, 3, 8, 2 };
		int max = Integer.MIN_VALUE;
		for (int su : array) {
			if (su > max) {
				max = su;
			}
		}
		System.out.println(max);
	}
}
