package Coding;

public class 오장8번 {

	public static void main(String[] args) {
		f1();
	}

	private static void f1() {
		int[][] array = {
				{95, 86},
				{83, 92, 96},
				{78, 83, 93, 87, 88}};
		
		int total = 0;
		int cnt = 0;
		
		for (int[] datas : array) {
			for(int data : datas) {
				cnt++;
				total += data;
			}
		}
		System.out.println("총점은 :"+total);
		System.out.println("평균은 :"+(double)total/cnt);
	}

}
