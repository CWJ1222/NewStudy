package com.shinhan.day02;

public class 반복문 {

	public static void main(String[] args) {
		// call();
		// call2();
		// call3();
		// call4();
		// call5();
		call6();

	}

	private static void call6()
			// 1~5 출력 
			int su =1;
			do {
				System.out.println(su);
				su++;

			}while(su>=5);
		}

	private static void call5()
		// 1~5 출력 
		int su =1;
		while(su<=5) {
			System.out.println(su);
			su++;
		}
	}

	private static void call4() {
		// 1~5 출력
		for (int su = 1; su <= 5; su++) {
			System.out.println(su);

		}
	}

	private static void call3() {
		int total = 0, total2 = 0;
		for (int su = 1, su2 = 5; su <= 100; su++, su2 += 5) {
			total += su;
			total2 += su2;
		}
		System.out.println(total);
		System.out.println(total2);
	}

	private static void call2() { // 1~100 합 구하기
		int total = 0;
		for (int su = 1; su <= 100; su++) {
			total += su;
		}
		System.out.println(total);
	}

	private static void call() {
		// 1. for(초기값;조건;증감)문, 같은 구문을 반복할 경우, 다생략하면 무한 반복.
		int i;
		i = 1;
		for (;;) {
			System.out.printf("----%d-------\n", i);

			if (i == 10)
				break;
			i++;
		}

	}

}
/*
 * 
 * int i; for(i=1;i<=10;i++){ {System.out.println("------"+i); }
 * System.out.println("i="+i); }
 * 
 * 
 */